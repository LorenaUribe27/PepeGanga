{
  "store.home": {
    "blocks": [
      "rich-text#title-shelve-1",
      "flex-layout.row#shelf-1",

      "rich-text#title-shelve-2",
      "flex-layout.row#shelf-2",


      "rich-text#title-shelve-tabs",
      "flex-layout.row#tabsHomeShelfs",

      "rich-text#title-brands",
      "flex-layout.row#bannersMarca"
    ]
  },
 
  "rich-text#title-shelve-1": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "### SÃºper descuentos",
      "textColor": "black-50",
      "blockClass": "block-title-p1"
    }
  },
  "flex-layout.row#shelf-1": {
    "children": ["list-context.product-list#shelve-1"],
    "props": {
      "blockClass": "shelf--container"
    }
  },
  "list-context.product-list#shelve-1": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#shelve-products-1"],
    "props": {
      "orderBy": "OrderByTopSaleDESC"
    }
  },
  "slider-layout#shelve-products-1": {
    "props": {
      "itemsPerPage": {
        "desktop": 5,
        "tablet": 3,
        "phone": 2
      },
      "infinite": true,
      "fullWidth": true,
      "blockClass": "shelf--container"
    }
  },
  "rich-text#title-shelve-2": {
    "props": {
